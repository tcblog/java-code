<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--        namespace：名称空间-->
<mapper namespace="com.itheima.mapper.BooksMapper">
<!--    resultType 返回值类型-->
<!--    id  唯一标识-->
    <select id="selectAll" resultType="com.itheima.book.Book">
        select * from books;
    </select>
</mapper>